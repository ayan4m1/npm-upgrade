{
  "name": "@ayan4m1/npm-upgrade",
  "version": "4.2.3",
  "description": "Interactive CLI utility to easily update outdated NPM dependencies",
  "author": "Yuriy Grunin <grunin.ya@ya.ru>",
  "contributors": [
    {
      "name": "ayan4m1",
      "email": "andrew@bulletlogic.co"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/ayan4m1/npm-upgrade",
  "changelog": "https://github.com/ayan4m1/npm-upgrade/blob/master/CHANGELOG.md",
  "bugs": {
    "url": "https://github.com/ayan4m1/npm-upgrade/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayan4m1/npm-upgrade.git"
  },
  "type": "module",
  "module": "./lib/index.js",
  "bin": {
    "npm-upgrade": "lib/index.js"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint ./src/**/*.js",
    "prepare": "husky",
    "version": "auto-changelog -p -b 10 -l 10 && git add CHANGELOG.md"
  },
  "engines": {
    "node": ">=16"
  },
  "preferGlobal": true,
  "dependencies": {
    "@inquirer/prompts": "^7.4.0",
    "chalk": "^5.4.1",
    "cli-table": "^0.3.11",
    "commander": "^13.1.0",
    "detect-indent": "^7.0.1",
    "jsonfile": "^6.1.0",
    "libnpmconfig": "^1.2.1",
    "lodash": "^4.17.21",
    "npm-check-updates": "^16.14.20",
    "open": "^10.1.0",
    "pacote": "21.0.0",
    "semver": "^7.7.1",
    "shelljs": "^0.9.2"
  },
  "devDependencies": {
    "@ayan4m1/rollup-plugin-multi-input": "^2.1.2",
    "@babel/eslint-parser": "^7.26.10",
    "@rollup/plugin-terser": "^0.4.4",
    "auto-changelog": "^2.5.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import-x": "~4.9.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "rollup": "^4.36.0",
    "rollup-plugin-multi-input": "^1.5.0",
    "rollup-plugin-node-externals": "^8.0.0",
    "rollup-plugin-shebang-bin": "^0.1.0"
  },
  "files": [
    "lib",
    "db"
  ],
  "keywords": [
    "npm",
    "update",
    "outdated",
    "dependencies",
    "cli",
    "interactive",
    "automatic",
    "changelog",
    "ignore"
  ]
}
